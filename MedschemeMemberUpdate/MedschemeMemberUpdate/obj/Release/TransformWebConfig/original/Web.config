        <conversionPattern value="%date %level %logger - %message%newline" />
      </layout>
    </appender>
    <appender name="MyFileAppender" type="log4net.Appender.FileAppender">
      <file value="F:\Users\USER-PC\Documents\GitHub\MedschemeMemberUpdate\MedschemeMemberUpdate\MedschemeMemberUpdate\alog\pplication.log" />
